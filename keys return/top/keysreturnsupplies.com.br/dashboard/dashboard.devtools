        function updateTempoRestante() {
            const badges = document.querySelectorAll('[data-expires]');
            
            badges.forEach(badge => {
                const expiresAt = new Date(badge.dataset.expires);
                const now = new Date();
                const diff = expiresAt - now;
                
                if (diff > 0) {
                    const months = Math.floor(diff / (1000 * 60 * 60 * 24 * 30));
                    const days = Math.floor((diff % (1000 * 60 * 60 * 24 * 30)) / (1000 * 60 * 60 * 24));
                    const hours = Math.floor((diff % (1000 * 60 * 60 * 24)) / (1000 * 60 * 60));
                    const minutes = Math.floor((diff % (1000 * 60 * 60)) / (1000 * 60));
                    const seconds = Math.floor((diff % (1000 * 60)) / 1000);
                    
                    let tempoRestante = '';
                    if (months > 0) tempoRestante += months + 'm ';
                    if (days > 0) tempoRestante += days + 'd ';
                    tempoRestante += `${hours.toString().padStart(2, '0')}:${minutes.toString().padStart(2, '0')}:${seconds.toString().padStart(2, '0')}`;
                    badge.textContent = tempoRestante;
                    
                    if (hours === 0 && minutes === 0 && seconds === 0) {
                        badge.textContent = 'Expirado';
                        badge.className = 'inline-flex items-center px-2 py-1 bg-red-500/20 border border-red-500/50 rounded-full text-xs text-red-400';
                    }
                } else {
                    badge.textContent = 'Expirado';
                    badge.className = 'inline-flex items-center px-2 py-1 bg-red-500/20 border border-red-500/50 rounded-full text-xs text-red-400';
                }
            });
        }

        setInterval(updateTempoRestante, 1000);
        updateTempoRestante();

